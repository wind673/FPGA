module TrafficLight
(
	input in_clk_s,    
	output [23:0]out_ShowData;
);

reg [23:0]ShowData; 
 

always @(clk_s)
begin  
	 Show_Data = clk_s%10000;	
 
	 
end 


/* 连接输出 ---------------------------*/ 
assign out_ShowData = ShowData; 
 
 
 
endmodule
